Option Explicit On


Module VariablesAndDataTypesAndStuff


    Sub Main()

        Dim LaffyTaffy As Integer
        Dim jellyBeans As Integer

        Console.WriteLine(jellyBeans)

        jellyBeans = 120

        Console.WriteLine(jellyBeans)

        jellyBeans = jellyBeans + 10

        Console.WriteLine(jellyBeans)

        jellyBeans += 10

        Console.WriteLine(jellyBeans)

        LaffyTaffy = 26

        Console.WriteLine(LaffyTaffy)
    End Sub

End Module
